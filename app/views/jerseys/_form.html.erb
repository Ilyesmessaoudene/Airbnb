<%= simple_form_for(@jersey) do |f| %>
  <%= f.input :team %>
  <%= f.input :player %>
  <%= f.input :year %>
    <%= f.input :state %>
  <%= f.input :description %>
  <%= f.input :photo %>
    <%= f.input :number %>
  <%= f.input :price_per_day %>
  <%= f.input :seller_address %>
    <%= f.input :size %>
  <%= f.submit %>
<% end %>
